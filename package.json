{
  "name": "solarized-flat-theme",
  "displayName": "Solarized Flat Theme",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/elling19/vscode-extension-solarized-flat-theme"
  },
  "publisher": "Elling",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "python",
        "extensions": [
          ".py"
        ]
      },
      {
        "id": "java",
        "extensions": [
          ".java"
        ]
      }
    ],
    "grammars": [
      {
        "scopeName": "source.python.gs",
        "path": "./syntaxes/gs-python.tmLanguage.json",
        "injectTo": [
          "source.python"
        ],
        "languaige": "python"
      },
      {
        "scopeName": "source.java.gs",
        "path": "./syntaxes/gs-java.tmLanguage.json",
        "injectTo": [
          "source.java"
        ],
        "language": "java"
      }
    ],
    "themes": [
      {
        "label": "Solarized Flat Light",
        "uiTheme": "vs",
        "path": "./themes/Solarized Flat Light-color-theme.json"
      },
      {
        "label": "Solarized Flat Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Solarized Flat Dark-color-theme.json"
      }
    ]
  }
}
