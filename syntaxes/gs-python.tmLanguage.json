{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Python Grammar Support",
  "scopeName": "source.python.gs",
  "injectionSelector": "L:source.python",
  "patterns": [
    {
      "match": "(?<=\\bdef\\s+)([a-zA-Z_][a-zA-Z0-9_]*)",
      "captures": {
        "1": {
          "name": "entity.name.function.python source.python.gs.function.definition.name"
        }
      }
    },
    {
      "match": "\\b(def)\\b",
      "captures": {
        "1": {
          "name": "storage.type.function.python source.python.gs.function.definition.def"
        }
      }
    }
  ]
}