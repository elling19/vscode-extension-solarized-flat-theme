    "tokenColors": [
        /**
         * Markup相关
         */
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#4B83CD"
            }
        },
        {
            "name": "Markup: Output",
            "scope": [
                "markup.output",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#D33682",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#6C71C4"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#93A1A1"
            }
        },
        {
			"name": "Markup Bold",
			"scope": [
				"markup.bold",
			],
			"settings": {
				"foreground": "#2AA198",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#859900",
				"fontStyle": "italic"
			}
		},
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#859900"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "constant.numeric",  // 数字常量
                "constant.character",  // 字符常量
                "constant.other",  // 其他常量
                "constant.other.symbol",  // 符号常量
                "meta.method.return-type.java",  // Java方法返回类型
                "storage.modifier.package.java", // Java包名
            ],
            "settings": {
                "foreground": "#B58900",
            }
        },
        {
            "name": "Green",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language",
                "keyword.operator",
                "storage.type",
                "support.type",
                "entity.name.tag.html", // HTML标签名
				"entity.name.tag.style", // style标签
				"entity.name.tag.script", // script标签
                "support.function.magic.python",  // Python魔法函数
                "support.variable.magic.python",  // Python魔法变量
                "variable.parameter.function.language.special.self.python", // Python的self参数
                "storage.type.class.python", // python 类定义关键字
                "storage.type.function.python", // python 函数定义关键字
                "storage.type.java", // Java类型定义关键字
                "keyword.other.package.java",  // Java包导入关键字
				"keyword.other.import.java",  // Java导入关键字
                "storage.type.local.java", // Java局部变量定义关键字:var
                "storage.type.primitive.java", // Java基本类型关键字
                "storage.type.object.array.java", // Java数组类型关键字
                "storage.modifier.java", // Java修饰符
                "storage.type.function.arrow.ts", // TS箭头函数关键字
                "keyword.other.directive.using.cs", // C# using指令关键字
				"storage.modifier"
            ],
            "settings": {
                "foreground": "#859900",
            }
        },
        {
            "name": "Magenta",
            "scope": [
                "variable.parameter",
                "meta.function.parameters",
                // 类相关
                "entity.name.type.class",  // 类
                "entity.name.type.interface", // 接口
                "entity.other.inherited-class", // 继承的类名
                "meta.enum", // 枚举名
                // 函数相关
                "meta.definition.function", // 函数定义
                "meta.function.python",  // Python函数
                "meta.method.identifier.java", // Java方法名，参数类型
                "entity.name.tag.template.html.vue", // Vue模板标签名
            ],
            "settings": {
                "foreground": "#D33682",
            }
        },
        {
            "name": "Blue",
            "scope": [
                "keyword", // 关键字
                "string.regexp",  // 正则表达式
                "meta.function-call", // 函数调用
                "meta.method-call", // 方法调用
                "entity.other.attribute-name.html", // HTML属性名
                "meta.method-call.java",
                "variable.parameter.java", // Java函数参数
                "storage.type.annotation.java",  // Java注解类型关键字
				"entity.name.type.annotation.java",  // Java注解名
				"variable.annotation.java", // Java注解变量
                "source.ts.embedded.html.vue", // Vue文件中的TS代码
                "entity.other.attribute-name.class.css", // css class属性
                "entity.other.attribute-name.id.css", // css id属性
                "entity.name.tag.css", // 内置css属性名,例如div、p 
				"entity.name.type.namespace",  // 命名空间
				"entity.name.function.cs", // C#函数名
            ],
            "settings": {
                "foreground": "#268BD2",
            }
        },
        {
            "name": "Violet",
            "scope": [
                "support.variable",  // 系统变量
                "keyword.control", // 控制关键字
                "variable.object.property", // 变量对象属性
                "variable.other.property", // 其他对象属性
                "variable.other.object.property", // 其他变量对象属性
                "meta.attribute.python", // Python属性
                "meta.object-literal.key.ts", // TS对象属性
                "support.type.property-name.css", // CSS属性名
            ],
            "settings": {
                "foreground": "#6C71C4",
            }
        },
        {
            "name": "Orange",
            "scope": [
                "punctuation.definition.decorator.python", // Python装饰器@符号
                "meta.function.decorator.python", // Python装饰器函数
                "storage.type.generic.java", // Java泛型类型关键字
				"punctuation.definition.annotation.java", // Java注解@符号
                "storage.type.annotation.java",  // Java注解类型关键字
            ],
            "settings": {
                "foreground": "#CB4B16",
            }
        },
        {
            "name": "Cyan",
            "scope": [
                "string.quoted.single", // 单引号字符串
                "string.quoted.double", // 双引号字符串
            ],
            "settings": {
                "foreground": "#2AA198",
            }
        },
        {
            "name": "Red",
            "scope": [
            ],
            "settings": {
                "foreground": "#DC322F",
            }
        },
        {
			"name": "Gray",
			"scope": [
                "comment",
                "punctuation.separator",
                "punctuation.accessor", // 访问符号.
                "punctuation.definition.tag.begin.html", // HTML标签开始符号<
                "punctuation.definition.tag.end.html", // HTML标签结束符号>
                "punctuation.terminator.rule", // css/scss语句结束分号
                "punctuation.definition.comment",  // 注释符号
				"string.quoted.docstring.multi.python",  // Python文档注释
                "constant.character.escape",  // 转义字符
                "punctuation.terminator.java", // Java语句结束分号
                "punctuation.section.function.begin.python", // python函数开始冒号
				"punctuation.terminator.statement.cs", // C#语句结束分号
			],
			"settings": {
				"foreground": "#586E75"
			}
		},
        {
			"name": "Foreground Default",  // 默认前景色
			"scope": [
            ],
			"settings": {
			}
		}
    ]
}