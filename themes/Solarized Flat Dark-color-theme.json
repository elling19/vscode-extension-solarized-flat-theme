    "tokenColors": [
        {
            "name": "Parentheses",
            "scope": [
                "punctuation.parenthesis",
                "meta.brace.round",
                "punctuation.section.group",
                "punctuation.definition.parameters"
            ],
            "settings": {
                "foreground": "#93A1A1"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#586e75"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#586e75"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"  // 文档注释
            ],
            "settings": {
                "foreground": "#2AA198"
            }
        },
        {
            "name": "Diff / Git: Added",
            "scope": [
                "markup.inserted",
                "markup.inserted.diff",
                "punctuation.definition.inserted"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Diff / Git: Modified",
            "scope": [
                "markup.changed",
                "markup.changed.diff"
            ],
            "settings": {
                "foreground": "#b58900"
            }
        },
        {
            "name": "Diff / Git: Deleted",
            "scope": [
                "markup.deleted",
                "markup.deleted.diff",
                "punctuation.definition.deleted"
            ],
            "settings": {
                "foreground": "#dc322f"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Keywords", // 关键字
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",  // 自定义变量
            ],
            "settings": {
                "foreground": "#839496"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "support.variable"  // 系统变量
            ],
            "settings": {
                "foreground": "#6C71C4"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function", // 函数声明
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "support.function" // 函数调用
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Function Parameter", // 函数参数
            "scope": [
                "variable.parameter",
                "meta.function.parameters"
            ],
            "settings": {
                "foreground": "#D33682"
            }
        },
        {
            "name": "Classes", // 类
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",  // 数字常量
                "constant.character",  // 字符常量
                "constant.other",  // 其他常量
            ],
            "settings": {
                "foreground": "#B58900"
            }
        },
        {
            "name": "Strings",
            "scope": "string", // 字符串常量
            "settings": {
                "foreground": "#2AA198"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",  // 转义字符
            "settings": {
                "foreground": "#93A1A1"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp", // 正则表达式
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#B58900"  // 符号常量
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#839496" // 标点符号
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#91B3E0"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#D33682"
            }
        },
        {
            "name": "HTML: Entities",
            "scope": [
                "constant.character.entity",
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#AB6526"
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css",
                "meta.selector.css",
                "entity.name.tag.scss",
                "entity.name.tag.sass",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css"
            ],
            "settings": {
                "foreground": "#D33682"
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name",
                "support.type.property-name.css",
                "meta.property-name.scss",
                "meta.property-name.sass"
            ],
            "settings": {
                "foreground": "#2AA198"
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": [
                "meta.property-value",
                "meta.property-value constant.other",
                "support.constant.property-value",
                "meta.property-value.scss",
                "meta.property-value.sass",
                "meta.property-value.css",
                "support.constant.property-value.css"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#4B83CD"
            }
        },
        {
            "name": "Markup: Output",
            "scope": [
                "markup.output",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#D33682",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#6C71C4"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#93A1A1"
            }
        },
        {
			"name": "Markup Blod",
			"scope": [
				"markup.bold",
			],
			"settings": {
				"foreground": "#2AA198",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#859900",
				"fontStyle": "italic"
			}
		},
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#859900"
            }
        },
        {
            "name": "String Quotes",
            "scope": [
                "string.quoted.single punctuation.definition.string.begin",
                "string.quoted.single punctuation.definition.string.end",
                "string.quoted.double punctuation.definition.string.begin",
